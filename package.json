{
    "name": "@apitree/alda",
    "description": "React stack with material-ui",
    "version": "0.1.0-alpha.23",
    "author": "ApiTree",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ApiTreeCZ/alda.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ApiTreeCZ/alda/issues"
    },
    "homepage": "https://github.com/ApiTreeCZ/alda#readme",
    "scripts": {
        "generate:lang": "ts-node --compilerOptions {\\\"module\\\":\\\"commonjs\\\"} ./scripts/default-lang.ts",
        "generate:graphql:merge": "node -e \"console.log(require('graphql-import').importSchema('./schema/root.graphql'))\" > graphql.schema.graphql",
        "generate:graphql:json": "npm run generate:graphql:merge && apollo-codegen introspect-schema graphql.schema.graphql --output graphql.schema.json",
        "generate:graphql:typescript": "gql-gen --file graphql.schema.json --template typescript --out ./types/graphql.d.ts",
        "prebuild": "rimraf -- dist .next",
        "build": "npm run generate:graphql:json && npm run generate:graphql:typescript && next build && tsc --project tsconfig.server.json && npm run generate:lang",
        "start": "NODE_ENV=production node dist/index.js",
        "dev": "npm run generate:graphql:json && npm run generate:graphql:typescript && npm run generate:lang && nodemon server/index.ts",
        "debug": "node --inspect dist/index.js",
        "tslint": "tslint -c tslint.json 'client/**/*.*' 'pages/**/*.*' 'server/**/*.*'",
        "tslint:check": "tslint-config-prettier-check ./tslint.json",
        "prettier:show": "prettier --list-different \"./**/*\"",
        "prettier:write": "prettier \"./**/*\" --write",
        "precommit": "pretty-quick --staged && npm run tslint",
        "test": "jest",
        "coverage": "jest --coverage",
        "coverage:report": "npm run coverage && cat ./coverage/lcov.info | coveralls"
    },
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "collectCoverageFrom": [
            "client/**/*.ts"
        ]
    },
    "dependencies": {
        "@material-ui/core": "1.2.0",
        "@material-ui/icons": "1.1.0",
        "accepts": "1.3.5",
        "apollo-boost": "0.1.7",
        "apollo-server-express": "1.3.6",
        "autoprefixer": "8.6.0",
        "body-parser": "1.18.3",
        "dotenv": "6.0.0",
        "express": "4.16.3",
        "glob": "7.1.2",
        "graphql": "0.13.2",
        "graphql-import": "0.6.0",
        "graphql-tag": "2.9.2",
        "graphql-tools": "3.0.2",
        "intl": "1.2.5",
        "isomorphic-fetch": "2.2.1",
        "next": "6.0.3",
        "next-redux-wrapper": "2.0.0-beta.6",
        "nprogress": "0.2.0",
        "postcss": "6.0.22",
        "react": "16.4.0",
        "react-apollo": "2.1.4",
        "react-dom": "16.4.0",
        "react-intl": "2.4.0",
        "react-redux": "5.0.7",
        "redux": "4.0.0",
        "redux-actions": "2.4.0",
        "redux-form": "7.3.0",
        "redux-thunk": "2.3.0",
        "reselect": "3.0.1"
    },
    "devDependencies": {
        "@playlyfe/gql": "2.6.0",
        "@types/accepts": "1.3.5",
        "@types/autoprefixer": "6.7.3",
        "@types/express": "4.16.0",
        "@types/glob": "5.0.35",
        "@types/intl": "1.2.0",
        "@types/isomorphic-fetch": "0.0.34",
        "@types/jest": "22.2.3",
        "@types/next": "2.4.11",
        "@types/node": "10.3.1",
        "@types/nprogress": "0.0.29",
        "@types/react": "16.3.16",
        "@types/react-intl": "2.3.8",
        "@types/react-redux": "6.0.2",
        "@types/redux-actions": "2.3.0",
        "@types/redux-form": "7.2.6",
        "@zeit/next-typescript": "1.0.1",
        "apollo-codegen": "0.19.1",
        "babel-plugin-react-intl": "2.4.0",
        "coveralls": "3.0.1",
        "graphql-code-generator": "0.9.1",
        "graphql-codegen-typescript-template": "0.9.1",
        "husky": "0.14.3",
        "jest": "22.4.4",
        "nodemon": "1.17.5",
        "prettier": "1.13.4",
        "pretty-quick": "1.6.0",
        "rimraf": "2.6.2",
        "ts-jest": "22.4.6",
        "ts-node": "6.1.0",
        "tslint": "5.10.0",
        "tslint-config-prettier": "1.13.0",
        "tslint-react": "3.6.0",
        "typescript": "2.9.1"
    }
}
