{
  "name": "@apitree/alda",
  "description": "React stack with material-ui",
  "version": "0.1.0-alpha.9",
  "author": "ApiTree",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApiTreeCZ/alda.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ApiTreeCZ/alda/issues"
  },
  "homepage": "https://github.com/ApiTreeCZ/alda#readme",
  "scripts": {
    "prebuild": "rimraf -- dist .next",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "nodemon server/index.ts",
    "tslint": "tslint -c tslint.json 'client/**/*.*' 'pages/**/*.*' 'server/**/*.*'",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier:show": "prettier --list-different \"./**/*\"",
    "prettier:write": "prettier \"./**/*\" --write",
    "precommit": "pretty-quick --staged && npm run tslint",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "client/**/*.ts"
    ]
  },
  "dependencies": {
    "@material-ui/icons": "1.0.0-beta.43",
    "apollo-client-preset": "1.0.8",
    "apollo-link-error": "1.0.9",
    "autoprefixer": "8.4.1",
    "cssnano": "3.10.0",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "material-ui": "1.0.0-beta.45",
    "next": "5.1.0",
    "next-redux-wrapper": "1.3.5",
    "nprogress": "0.2.0",
    "postcss": "6.0.22",
    "react": "16.3.2",
    "react-apollo": "2.1.4",
    "react-dom": "16.3.2",
    "react-intl": "2.4.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-actions": "2.3.2",
    "redux-form": "7.3.0",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.1"
  },
  "devDependencies": {
    "@types/express": "4.11.1",
    "@types/jest": "22.2.3",
    "@types/next": "2.4.9",
    "@types/next-redux-wrapper": "1.4.0",
    "@types/node": "10.0.4",
    "@types/nprogress": "0.0.29",
    "@types/react": "16.3.13",
    "@types/react-intl": "2.3.8",
    "@types/redux-actions": "2.2.4",
    "@types/redux-form": "7.2.5",
    "@zeit/next-typescript": "0.1.1",
    "coveralls": "3.0.1",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "nodemon": "1.17.4",
    "prettier": "1.12.1",
    "pretty-quick": "1.4.1",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.5",
    "ts-node": "6.0.3",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.12.0",
    "tslint-react": "3.5.1",
    "typescript": "2.8.3"
  }
}
